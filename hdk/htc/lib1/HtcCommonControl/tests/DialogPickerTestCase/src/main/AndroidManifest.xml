<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.htc.dialogpicker.aut"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.SET_WALLPAPER_HINTS" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:allowBackup="true"
        android:vmSafeMode="true"
        android:allowClearUserData="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".AlertDialogDemo"
            android:configChanges="screenSize|orientation" >
        </activity>
        <activity
            android:name=".DataActionPickerDemo"
            android:configChanges="screenSize|orientation" >
        </activity>
        <activity
            android:name=".DatePickerDemo"
            android:configChanges="screenSize|orientation" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TimePickerDemo"
            android:configChanges="screenSize|orientation" >
        </activity>
        <activity
            android:name=".HtcMultiSeekBarDialogDemo"
            android:configChanges="orientation|screenSize" />
        <activity
            android:name=".ColorPickerAut"
            android:configChanges="orientation|screenSize" />
    </application>

</manifest>
