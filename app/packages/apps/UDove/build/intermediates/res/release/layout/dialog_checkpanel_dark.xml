<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:htc="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >
    <!-- below top padding is set for case of no text message -->
    <View android:id="@+id/checkBoxDivider"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@drawable/inset_list_divider_dark"
        android:visibility="gone"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/margin_l"
        android:paddingRight="@dimen/margin_l"
        android:gravity="center_vertical"
        android:orientation="horizontal" >
        <com.htc.lib1.cc.widget.HtcCheckBox
            android:id="@android:id/checkbox"
            android:paddingTop="@dimen/margin_l"
            android:paddingBottom="@dimen/margin_l"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            htc:backgroundMode="automotiveDark" />
        <TextView
            android:id="@android:id/text1"
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:paddingLeft="@dimen/margin_s"
            style="@style/fixed_darklist_secondary_m" />
    </LinearLayout>
</FrameLayout>
