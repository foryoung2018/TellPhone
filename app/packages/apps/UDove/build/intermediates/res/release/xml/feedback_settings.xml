<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:title="@string/feedback_error_report_title"
        android:key="error_report_category">

        <CheckBoxPreference
            android:key="error_report_enable"
            android:title="@string/feedback_error_report_setting"
            android:summary="@string/feedback_setting_summary"
            android:persistent="true"
            android:disableDependentsState="true" />

        <ListPreference
            android:key="auto_send_preference"
            android:title="@string/auto_send_preference_title"
            android:persistent="true"
            android:entries="@array/auto_send_preference_entries"
            android:entryValues="@array/auto_send_preference_values" />

    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/feedback_application_log_title"
        android:key="application_log_category">

        <CheckBoxPreference
            android:key="application_log_enable"
            android:title="@string/feedback_application_log_setting"
            android:summary="@string/feedback_application_log_summary"
            android:persistent="true"
            android:disableDependentsState="true" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/network_preference_title"
        android:key="network_preference_category">
        <ListPreference
            android:key="network_preference"
            android:title="@string/feedback_network_setting"
            android:persistent="true"
            android:entries="@array/network_preference_entries"
            android:entryValues="@array/network_preference_values" />
    </PreferenceCategory>

</PreferenceScreen>
